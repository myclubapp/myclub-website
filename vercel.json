{
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=0, must-revalidate"
          }
        ]
      }
    ],
    "trailingSlash": false,
    "cleanUrls": true,
    "routes": [
      {
        "src": "/terms-and-conditions-de",
        "dest": "/index.html"
      },
      {
        "src": "/terms-and-conditions",
        "dest": "/index.html"
      },
      {
        "src": "/privacy-policy-de",
        "dest": "/index.html"
      },
      {
        "src": "/privacy-policy",
        "dest": "/index.html"
      }
    ]
}